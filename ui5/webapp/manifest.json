{
	"_version": "1.8.0",
	"sap.app": {
	  "id": "hxeneo",
	  "type": "application",
	  "i18n": "i18n/i18n.properties",
	  "title": "{{appTitle}}",
	  "description": "{{appDescription}}",
	  "applicationVersion": {
		"version": "1.0.0"
	  }
	},
	"sap.ui": {
	  "technology": "UI5",
	  "deviceTypes": {
		  "desktop": true,
		  "tablet": true,
		  "phone": true
	  }
	},
	"sap.ui5": {
	  "rootView": {
		  "viewName": "hxeneo.view.S0",
		  "type": "XML"
	  },
	  "dependencies": {
		"minUI5Version": "1.30",
		"libs": {
		  "sap.m": {}
		}
	  },
	  "models": {
		"i18n": {
		  "type": "sap.ui.model.resource.ResourceModel",
		  "settings": {
			"bundleName": "hxeneo.i18n.i18n"
		  }
		}
	  },
	  "resources": {
		  "css": []
	  },
	  "routing": {
		  "config": {
			  "routerClass": "sap.m.routing.Router",
			  "viewPath": "hxeneo.view",
			  "controlId": "root",
			  "controlAggregation": "masterPages",
			  "clearAggregation": true,
			  "viewType": "XML",
			  "bypassed": {
				  "target": ["categories"]
			  }
		  },
		  "targets": {
		       	"categories": {
						"viewName": "S1"
					},
					"products": {
						"viewName": "S2",
						"controlAggregation": "detailPages"
					},
					"createCategory":{
						"viewName": "S3",
						"controlAggregation": "detailPages"
					},
					"productDetail": {
						"viewName": "S4",
						"controlAggregation": "detailPages"
					}
		  },
		  "routes": [{
			  "name": "default",
			  "pattern": "",
			  "target": ["categories"]
		  }, {
			  "name": "products",
			  "pattern": "category/{category_id}",
			  "target": ["products", "categories"]
		  },{
			  "name": "createCategory",
			  "pattern": "registerCatalog",
			  "target": ["createCategory"]
		  },{
			  "name": "productDetail",
			  "pattern": "productDetail/{product_id}",
			  "target": ["productDetail", "categories"]
		  }]
	  }
	}
  }